{
  "buildCommand": "cd client && npm run build",
  "outputDirectory": "client/build",
  "installCommand": "cd client && npm install",
  "framework": null,
  "env": {
    "REACT_APP_API_URL": "/api"
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://mern-backend.onrender.com/api/$1"
    }
  ]
}